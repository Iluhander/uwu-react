{
  "name": "@iluhander/uwu-react",
  "version": "1.6.0",
  "description": "Simple and lightweight react lib",
  "types": "./lib/esm/types/index.d.ts",
  "main": "./lib/esm/index.js",
  "type": "module",
  "exports": {
    "typesVersions": {
      "*": {
        "enums": "./lib/esm/types/src/enums/index.d.ts",
        "forms": "./lib/esm/types/src/forms/index.d.ts",
        "handlers": "./lib/esm/types/src/handlers/index.d.ts",
        "hooks": "./lib/esm/types/src/hooks/index.d.ts",
        "objs": "./lib/esm/types/src/objs/index.d.ts"
      }
    },
    "./enums": "./lib/esm/src/enums/index.js",
    "./forms": "./lib/esm/src/forms/index.js",
    "./handlers": "./lib/esm/src/handlers/index.js",
    "./hooks": "./lib/esm/src/hooks/index.js",
    "./objs": "./lib/esm/src/objs/index.js"
  },
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "clean": "rm -rf ./lib",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "build": "npm run clean && npm run build:esm",
    "build:esm": "tsc -p ./ts/tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
    "prepack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Iluhander/uwu-react.git"
  },
  "keywords": [
    "react",
    "http",
    "hooks",
    "uwu"
  ],
  "author": "iluhander",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Iluhander/uwu-react/issues"
  },
  "homepage": "https://github.com/Iluhander/uwu-react#readme",
  "devDependencies": {
    "@types/react": "^18.2.31",
    "husky": "^8.0.3",
    "jest": "^29.7.0"
  },
  "dependencies": {
    "react": "^18.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "CI=true npm run test"
    }
  }
}
